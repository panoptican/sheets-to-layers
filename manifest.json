{
  "name": "Sheets to Layers",
  "id": "sheets-sync-plugin",
  "api": "1.0.0",
  "main": "dist/code.js",
  "ui": "dist/ui.html",
  "editorType": ["figma"],
  "documentAccess": "dynamic-page",
  "networkAccess": {
    "allowedDomains": [
      "*"
    ],
    "reasoning": "This plugin fetches images from URLs specified in Google Sheets. Since users can reference images hosted on any domain, access to all domains is required."
  },
  "relaunchButtons": [
    {
      "command": "open",
      "name": "Open Sheets to Layers"
    },
    {
      "command": "resync",
      "name": "Re-Sync Google Sheets Data"
    }
  ]
}
